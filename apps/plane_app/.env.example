# Plane Mobile App â€” Environment Variables
# Copy this file to .env and fill in the values.

# Environment: dev | staging | production
ENV=dev

# Plane API base URL
# Cloud: https://api.plane.so
# Self-hosted: https://your-instance.example.com/api
BASE_URL=https://api.plane.so

# Enable debug logging (true/false)
ENABLE_LOGGING=true

# Sync engine interval in minutes
SYNC_INTERVAL_MINUTES=5

# --- Android Signing (release builds) ---
# KEYSTORE_PATH=path/to/keystore.jks
# KEYSTORE_PASSWORD=
# KEY_ALIAS=
# KEY_PASSWORD=

# --- App Store / Play Store ---
# APPLE_ID=developer@plane.so
# APPLE_TEAM_ID=
# GOOGLE_PLAY_JSON_KEY=path/to/play-store-key.json
